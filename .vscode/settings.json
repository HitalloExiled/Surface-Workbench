// Place your settings in this file to overwrite default and user settings.
{
    "workbench.iconTheme": "vscode-icons",
    "files.exclude":
    {
        "**/*.config":           true,
        "**/.git":               true,
        "**/.hg":                true,
        "**/.svn":               true,
        "**/.vs":                true,
        "**/.DS_Store":          true,
        "**/CVS":                true,
        "**/node_modules":       true,
        "**/package-lock.json":  true,
        "server/start/**/*.js":  true,
        "server/start/**/*.map": true,
        "modules/**/*.js":       true,
        "modules/**/*.map":      true
    }
}